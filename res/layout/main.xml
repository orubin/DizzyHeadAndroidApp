<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout android:paddingLeft="5dip" android:id="@+id/relativeLayout1" android:layout_width="fill_parent" android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android" android:background="@drawable/background">
		<ImageButton 	 android:src="@drawable/dizzyhead"		android:layout_width="wrap_content"  android:id="@+id/textView1" android:layout_height="wrap_content"  android:background="#00000000" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:layout_marginTop="10dip"></ImageButton>
		<TextView 		 android:layout_height="wrap_content" android:id="@+id/title" android:text="Grandma" android:layout_below="@+id/textView1" android:layout_centerHorizontal="true" android:gravity="center" android:textColor="@color/red" android:layout_marginTop="3dip" android:layout_width="fill_parent" android:textSize="18dip"></TextView>
  		<ImageButton 	 android:id="@+id/info" 	android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/info" 		 android:background="#00000000"       android:onClick="info" 	android:layout_below="@+id/analysis" android:layout_marginTop="5dip"/>
		<ImageButton 	 android:id="@+id/settings" android:src="@drawable/settings" 	android:layout_width="wrap_content"  android:layout_height="wrap_content" android:background="#00000000" android:onClick="settings" android:layout_below="@+id/title"></ImageButton>
		<ImageButton 	 android:id="@+id/pencil" android:src="@drawable/pencil" 	android:layout_width="wrap_content"  android:layout_height="wrap_content" android:background="#00000000" android:onClick="pencil" android:layout_below="@+id/friends" android:layout_marginTop="5dip"></ImageButton>
		<ImageButton 	 android:id="@+id/analysis" android:src="@drawable/analysis" 	android:layout_width="wrap_content"  android:onClick="analysis"			  android:background="#00000000" android:layout_height="wrap_content" android:layout_below="@+id/pencil" android:layout_marginTop="5dip"></ImageButton>
		<ImageView       android:layout_height="wrap_content" android:layout_width="wrap_content" android:src="@drawable/bird1" android:id="@+id/face" android:layout_marginTop="10dip" android:layout_below="@+id/title" android:layout_centerHorizontal="true" android:layout_marginLeft="10dip"></ImageView>
		<ImageView       android:layout_height="wrap_content" android:layout_width="wrap_content" android:src="@drawable/friends" android:id="@+id/friends" android:layout_below="@+id/settings" android:onClick="friends" android:maxWidth="64px" android:maxHeight="64px" android:layout_marginTop="5dip"></ImageView>
		<ImageView 		 android:layout_height="wrap_content" android:id="@+id/imageView1" android:layout_width="wrap_content" android:layout_below="@+id/beer" android:src="@drawable/camaro" android:onClick="drive" android:layout_marginTop="-11dip"></ImageView>
		<EditText 	 	 android:id="@+id/time" 	android:layout_height="wrap_content" android:layout_width="wrap_content" android:background="#00000000" android:editable="false" android:gravity="center_horizontal" android:textColor="@color/red" android:layout_centerHorizontal="true" android:layout_below="@+id/sober" android:height="52dip" android:textSize="36dip" android:width="140dip" android:cursorVisible="false"></EditText>
		<TextView 		 android:textStyle="bold" android:layout_height="wrap_content" android:id="@+id/sober"   android:textColor="@color/blue" android:text="Time To Sober:" android:layout_centerHorizontal="true" android:layout_width="wrap_content" android:textSize="20dip" android:layout_marginTop="30dip" android:layout_below="@+id/analysis"></TextView>	
		<ImageButton 	 android:id="@+id/chaser"   android:src="@drawable/chaser"    android:background="#00000001"	android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/time"     android:onClick="Chaser" 		android:layout_toRightOf="@+id/shot" android:layout_marginTop="38dip"></ImageButton>
		<ImageButton 	 android:id="@+id/shot"     android:text="Shot"    android:src="@drawable/shot"	     android:background="#00000001" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/time"  	  android:onClick="Shot"		android:layout_centerHorizontal="true" android:layout_marginLeft="0dip" android:layout_toRightOf="@+id/wine" android:layout_marginTop="26dip"></ImageButton>
		<ImageButton     android:onClick="facebook" android:layout_height="wrap_content" android:src="@drawable/facebook" android:id="@+id/facebook" android:layout_width="wrap_content" android:background="#00000000" android:layout_below="@+id/shot" android:layout_alignRight="@+id/chaser" android:layout_marginTop="5dip"></ImageButton>
		<ImageButton 	 android:id="@+id/beer"     android:text="Beer"    android:src="@drawable/beer" android:background="#00000001"	android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/time" 	  android:onClick="beer" 		android:layout_marginTop="15dip" android:layout_marginLeft="15dip"></ImageButton>
		<ImageButton 	 android:id="@+id/wine"     android:text="Wine"	   android:src="@drawable/wine"		 android:background="#00000001" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/time" 	  android:onClick="wine" 		android:layout_toRightOf="@+id/beer" android:layout_marginLeft="10dip" android:layout_marginTop="15dip"></ImageButton>
    </RelativeLayout>